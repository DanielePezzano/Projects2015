<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <webHttpBinding>
                <binding name="UniverseBinding"/>
            </webHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:51822/Universe.svc" binding="webHttpBinding" bindingConfiguration="UniverseBinding"
                contract="UniverseWcf.IUniverse" name="UniverseTester" behaviorConfiguration="Tester.EndpointUniverse"
                kind="webHttpEndpoint" endpointConfiguration="">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
      <behaviors>
        <endpointBehaviors>
          <behavior name="Tester.EndpointUniverse">
            <webHttp automaticFormatSelectionEnabled="true" defaultBodyStyle="Wrapped"/>
          </behavior>
        </endpointBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>